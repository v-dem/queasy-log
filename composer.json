{
    "name": "v-dem/queasy-log",
    "description": "Logger classes (currently supports file system and console logging), part of Queasy PHP Framework",
    "homepage": "https://github.com/v-dem/queasy-log/",
    "keywords": ["log", "logger", "logging", "php", "psr-3"],
    "license": "LGPL-3.0-only",
    "version": "1.0.1",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Vitaly Demyanenko",
            "email": "vitaly_demyanenko@yahoo.com"
        }
    ],
    "config": {
        "preferred-install": "dist"
    },
    "require": {
        "php": ">=5.3.0|>=7.0.0",
        "psr/log": "~1.1"
    },
    "require-dev": {
        "php": ">=7.1.0",
        "phpunit/phpunit": "~7",
        "ext-xdebug": "^2.6.0"
    },
    "autoload": {
        "psr-4": {
            "queasy\\log\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "queasy\\log\\tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit --coverage-clover clover.xml"
    }
}

