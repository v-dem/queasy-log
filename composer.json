{
    "name": "v-dem/queasy-log",
    "description": "Logger classes, part of Queasy PHP framework",
    "keywords": ["file system logger", "console logger", "email logger"],
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Vitaly Demyanenko",
            "email": "vitaly_demyanenko@yahoo.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "psr/log": "~1.0",
        "v-dem/queasy-config": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "psr-4": {
            "queasy\\log\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "queasy\\log\\tests\\": "tests/"
        }
    }
}

